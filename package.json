{
  "name": "wayofjs",
  "version": "2.0.2",
  "description": "A library for managing models and views",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "prebuild": "rimraf dist",
    "clean": "rimraf dist",
    "compile": "tsc",
    "generate-dts": "node generate-dts.cjs",
    "bundle": "webpack --config webpack.config.js --mode=production",
    "postbuild:webpack": "npm run cleanup",
    "cleanup": "rimraf dist/tests dist/core dist/utils dist/router dist/index.js.LICENSE.txt",
    "build:webpack": "npm run prebuild && npm run compile && npm run generate-dts && npm run bundle && npm run cleanup",
    "test": "jest",
    "build": "npx tsc && node fix-import.cjs"
  },
  "author": "Vikash Kumar",
  "license": "MIT",
  "keywords": [
    "models",
    "views",
    "library",
    "javascript",
    "typescript"
  ],
  "dependencies": {
    "axios": "^1.7.2",
    "browserify-fs": "^1.0.0",
    "buffer": "^6.0.3",
    "ejs": "^3.1.10",
    "htm": "^3.1.1",
    "path-browserify": "^1.0.1",
    "stream-http": "^3.2.0",
    "url": "^0.11.3"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@babel/plugin-transform-modules-commonjs": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-typescript": "^7.24.6",
    "@types/ejs": "^3.1.5",
    "@types/jest": "^29.5.12",
    "@types/path-browserify": "^1.0.2",
    "axios-mock-adapter": "^1.22.0",
    "babel-jest": "^29.7.0",
    "clean-webpack-plugin": "^4.0.0",
    "dts-bundle-generator": "^9.5.1",
    "esm": "^3.2.25",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-extended": "^4.0.2",
    "jest-transform-stub": "^2.0.0",
    "jsdom": "^24.1.0",
    "process": "^0.11.10",
    "replace-in-file": "^7.2.0",
    "rimraf": "^5.0.7",
    "terser-webpack-plugin": "^5.3.10",
    "ts-jest": "^29.1.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  }
